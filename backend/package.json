{
  "name": "tecnoforum_backend",
  "version": "1.0.0",
  "engines": {
    "node": "12.16.3",
    "npm": "6.14.4"
  },
  "description": "backend for tecnoforum",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "npm install && cd ./frontend/ && npm install && npm run build",
    "dev": "nodemon index.js",
    "lint": "eslint .",
    "test": "mocha",
    "quick": "cd ./frontend/ && npm run win-build && cd ../ && npm run dev"
  },
  "cacheDirectories": [
    "node_modules",
    "frontend/node_modules"
  ],
  "keywords": [
    "nodejs",
    "express",
    "axios",
    "mongoose"
  ],
  "author": "Esa Maaninka",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^1.0.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.24",
    "mongoose-paginate": "^5.0.3",
    "mongoose-type-email": "^1.0.12",
    "mongoose-unique-validator": "^2.0.3",
    "validator": "^13.1.1",
    "react-csp": "^1.1.0"
  }
}
